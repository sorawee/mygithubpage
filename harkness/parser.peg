Arguments
  = _ head:Atom tail:(__ Atom)* _ { return [head].concat(tail.map(e => e[1])); } /
  _ { return []; }

Atom
  = Value / String

Value "value"
  = val:[^" ]+ {
    return val.join("");
  }

String
  = "\"" val:StringContent* "\"" {
    return val.join("");
  }

StringContent "string content" = [^"]

_ "whitespace"
  = [ ]*

__ "mandatory whitespace"
  = [ ]+